{
	"name": "proto-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview-all": "npm run wasm && npm run build && wrangler pages dev",
		"preview": "npm run build && wrangler pages dev",
		"wrglr-preview": "wrangler pages dev",
		"wrglr-deploy": "wrangler pages deploy",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"deploy-all": "npm run wasm && npm run build && wrangler pages deploy",
		"deploy": "npm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
		"wasm": "wasm-pack build ./proto-query-engine --target web",
		"install-rust-wasm": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && . $HOME/.cargo/env && rustup toolchain install stable --profile minimal && rustup target add wasm32-unknown-unknown && cargo install wasm-pack",
		"surreal-fix": "cp node_modules/surrealdb.wasm/dist/embedded/index_bg.wasm node_modules/.vite/deps/"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240903.0",
		"@playwright/test": "^1.47.0",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.26",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/wicg-file-system-access": "^2023.10.5",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"flowbite": "^2.5.1",
		"flowbite-svelte": "^0.46.16",
		"flowbite-svelte-icons": "^1.6.1",
		"flowbite-typography": "^1.0.3",
		"globals": "^15.9.0",
		"postcss": "^8.4.45",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.1",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.5.0",
		"vite": "^5.4.3",
		"vite-plugin-wasm": "^3.3.0",
		"vitest": "^2.0.5",
		"wrangler": "^3.75.0"
	},
	"type": "module",
	"dependencies": {
		"@apache-arrow/ts": "^17.0.0",
		"@codemirror/lang-sql": "^6.7.1",
		"@dagrejs/dagre": "^1.1.4",
		"@xyflow/svelte": "^0.1.18",
		"chart.js": "^4.4.4",
		"codemirror": "^6.0.1",
		"proto-query-engine": "file:../proto-query-engine/pkg",
		"surrealdb.wasm": "^1.0.0-beta.15"
	}
}
